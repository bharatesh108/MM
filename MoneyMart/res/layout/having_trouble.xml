<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv_having_trouble_customtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/margin_15dp"
        android:layout_marginLeft="@dimen/margin_15dp"
        android:layout_marginRight="@dimen/margin_15dp"
        android:layout_marginStart="@dimen/margin_15dp"
        android:layout_marginTop="@dimen/margin_15dp"
        android:fontFamily="sans-serif-light"
        android:textColor="@color/personal_details_data"
        android:textSize="@dimen/font_size_13" />
    <!-- android:text="@string/having_trouble_custom_text" -->

    <LinearLayout
        android:id="@+id/ll_having_trouble_phone_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_having_trouble_customtext"
        android:layout_marginTop="@dimen/margin_25dp"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/iv_having_trouble_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_15dp"
            android:layout_marginStart="@dimen/margin_15dp"
            android:background="@drawable/phone"
            android:contentDescription="@string/phone_content" />

        <TextView
            android:id="@+id/tv_having_trouble_phone_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_10dp"
            android:layout_marginStart="@dimen/margin_10dp"
            android:drawableLeft="@drawable/v_line"
            android:drawablePadding="@dimen/margin_15dp"
            android:drawableStart="@drawable/v_line"
            android:fontFamily="sans-serif-light"
            android:textColor="@color/actaul_text_color" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_having_trouble_hours_of_operations_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll_having_trouble_phone_layout"
        android:layout_marginTop="@dimen/margin_25dp"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/iv_having_trouble_hours_of_operations"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_15dp"
            android:layout_marginStart="@dimen/margin_15dp"
            android:background="@drawable/clock"
            android:contentDescription="@string/phone_content" />

        <TextView
            android:id="@+id/tv_having_trouble_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_10dp"
            android:layout_marginStart="@dimen/margin_10dp"
            android:drawableLeft="@drawable/v_line"
            android:drawablePadding="@dimen/margin_10dp"
            android:drawableStart="@drawable/v_line"
            android:fontFamily="sans-serif"
            android:textColor="@color/timings_text_color"/>

        <TextView
            android:id="@+id/tv_having_trouble_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_10dp"
            android:layout_marginStart="@dimen/margin_10dp"
            android:drawableLeft="@drawable/v_line"
            android:drawablePadding="@dimen/margin_10dp"
            android:drawableStart="@drawable/v_line"
            android:fontFamily="sans-serif"
            android:textColor="@color/personal_details_data"
            android:textSize="@dimen/font_size_13" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_having_trouble_mail_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll_having_trouble_hours_of_operations_layout"
        android:layout_marginTop="@dimen/margin_25dp"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/iv_having_trouble_mail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_15dp"
            android:layout_marginStart="@dimen/margin_15dp"
            android:background="@drawable/send_symbol"
            android:contentDescription="@string/mail_content" />

        <TextView
            android:id="@+id/tv_having_trouble_mail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_10dp"
            android:layout_marginStart="@dimen/margin_10dp"
            android:drawableLeft="@drawable/v_line"
            android:drawablePadding="@dimen/margin_15dp"
            android:drawableStart="@drawable/v_line"
            android:fontFamily="sans-serif-light"
            android:textColor="@color/actaul_text_color" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_having_trouble_weburl_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll_having_trouble_mail_layout"
        android:layout_marginTop="@dimen/margin_25dp"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/iv_having_trouble_weburl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_15dp"
            android:layout_marginStart="@dimen/margin_15dp"
            android:background="@drawable/web_site"
            android:contentDescription="@string/phone_content" />

        <TextView
            android:id="@+id/tv_having_trouble_weburl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_10dp"
            android:layout_marginStart="@dimen/margin_10dp"
           
            android:drawableLeft="@drawable/v_line"
            android:drawablePadding="@dimen/margin_15dp"
            android:drawableStart="@drawable/v_line"
            android:fontFamily="sans-serif-light"
            android:textColor="@color/actaul_text_color" />
        <!--  android:autoLink="web" -->
    </LinearLayout>

    <TextView
        android:id="@+id/tv_having_trouble_day_french"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_having_trouble_customtext"
        android:layout_alignLeft="@+id/tv_having_trouble_customtext"
        android:layout_below="@+id/ll_having_trouble_hours_of_operations_layout"
        android:layout_marginStart="@dimen/margin_35dp"
        android:layout_marginLeft="@dimen/margin_35dp"
        android:layout_toRightOf="@+id/tv_having_trouble_customtext"
        android:layout_toEndOf="@+id/tv_having_trouble_customtext"
        android:fontFamily="sans-serif"
        android:textColor="@color/personal_details_data"
        android:textSize="@dimen/font_size_13"
        android:visibility="gone" />

</RelativeLayout>